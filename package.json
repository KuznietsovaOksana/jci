    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-responsive": "^9.0.2",
    "react-use": "^17.4.0",
    "swiper": "^9.4.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
